# simulation
runmode : "simulate"
randseed: 2032
plastic : no
episodic: no
rpcport : "/stacs/rpc"
rpcpause: no

# network
netwkdir: "networks/dgnet"
netparts: 128 # 128 network partitions
netfiles: 128 # 128 network data files
filebase: "dgnet"
fileload: ""
filesave: ".out"
recordir: "record"
groupdir: "group"

# timing
tstep   : 1.0 # 1 millisecond
teventq : 10.0 # 10 milliseconds
tdisplay: 1000.0 # 1 second
trecord : 1000.0 # 1 second
tsave   : 5000.0 # 5 second
tmax    : 5000.0 # 5 second
